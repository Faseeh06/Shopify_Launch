<script>
document.addEventListener('DOMContentLoaded', function() {
  // Vanilla JS implementation of accordion
  var accKeys = document.querySelectorAll(".set > .open-acq");
  
  accKeys.forEach(function(el) {
    el.addEventListener('click', function() {
      var isActive = this.classList.contains('active');
      var content = this.nextElementSibling;
      var span = this.querySelector('span');

      // If clicked item is already active/open
      if (isActive) {
        this.classList.remove("active");
        if(content) content.style.display = 'none';
        
        // Reset icon
        if(span) {
          span.classList.remove("acc-minus");
          span.classList.add("acc-plus");
        }
      } else {
        // Close ALL other items first
        accKeys.forEach(function(item) {
          item.classList.remove("active");
          var ITEM_Content = item.nextElementSibling;
          if(ITEM_Content) ITEM_Content.style.display = 'none';
          
          var itemSpan = item.querySelector('span');
          if(itemSpan) {
            itemSpan.classList.remove("acc-minus");
            itemSpan.classList.add("acc-plus");
          }
        });

        // Open CURRENT item
        this.classList.add("active");
        if(content) content.style.display = 'block';
        
        // Set icon to minus
        if(span) {
           span.classList.remove("acc-plus");
           span.classList.add("acc-minus");
        }
      }
    });
  });
});
</script>

