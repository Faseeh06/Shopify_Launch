{%- style -%}
  .featuredcollections {
    margin-top: {{ section.settings.margin_top | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .featuredcollections {
      margin-top: {{ section.settings.margin_top }}px;
    }
  }

  .top-head {
    text-align: center;
    margin-bottom: 3rem;
    font-size: 2.8rem;
    font-weight: 600;
    color: #333;
  }

  .top-head em {
    font-style: normal;
    color: #e6b550;
  }

  .top-head span {
    color: #000;
  }

  .customproductgrid {
    display: grid;
    grid-template-columns: repeat(4, minmax(350px, 1fr));
    gap: 2rem;
    margin-bottom: 3rem;
  }

  .customgrid__item {
    position: relative;
    height: 100%;
  }

  .product-card__wrap {
    height: 100%;
    display: flex;
    flex-direction: column;
    background: white;
    border-radius: 8px;
    box-shadow: 0 2px 8px rgba(0,0,0,0.1);
    transition: box-shadow 0.3s ease;
    overflow: hidden;
  }

  .product-card__wrap:hover {
    box-shadow: 0 4px 16px rgba(0,0,0,0.15);
  }

  .product-card__badge__imag {
    position: absolute;
    top: 10px;
    left: 10px;
    z-index: 2;
  }

  .best-img-red {
    display: inline-block;
    width: 80px;
    height: 80px;
  }

  .product-card__badges {
    position: absolute;
    top: 10px;
    right: 10px;
    z-index: 2;
  }

  .product-card__badge {
    background: #e74c3c;
    color: white;
    padding: 4px 8px;
    border-radius: 4px;
    font-size: 0.875rem;
    font-weight: 600;
  }

  .product-card__info {
    padding: 1.5rem;
    height: 220px;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
  }

  .product-card__title {
    font-size: 1.1rem;
    font-weight: 600;
    margin-bottom: 0.5rem;
    line-height: 1.3;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
  }

  .product-card__subtitle {
    font-size: 0.9rem;
    color: #666;
    margin-bottom: 0.75rem;
    line-height: 1.4;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
  }

  .product-card__prices {
    display: flex;
    align-items: center;
    gap: 0.5rem;
    margin-top: 0.5rem;
  }

  .product-card__price-text {
    color: #666;
    font-size: 0.875rem;
  }

  .product-card__price {
    color: #e74c3c;
    font-weight: 600;
  }

  .product-card__compareprice {
    color: #999;
    text-decoration: line-through;
  }

  .viewalldiv {
    text-align: center;
    margin-top: 2rem;
  }

  .viewallbutton {
    display: inline-block;
    padding: 12px 30px;
    background: #e6b550;
    color: white;
    text-decoration: none;
    border-radius: 25px;
    font-weight: 600;
    transition: background 0.3s ease;
  }

  .viewallbutton:hover {
    background: #d4a549;
    color: white;
  }

  .custom-container {
    max-width: 1400px;
    margin: 0 auto;
    padding-left: 1rem;
    padding-right: 1rem;
  }

  @media (max-width: 749px) {
    .customproductgrid {
      grid-template-columns: repeat(1, 1fr);
      gap: 1rem;
    }

    .top-head {
      font-size: 2.2rem;
      margin-bottom: 2rem;
    }
  }

  @media (min-width: 750px) and (max-width: 1099px) {
    .customproductgrid {
      grid-template-columns: repeat(2, minmax(320px, 1fr));
      gap: 1.5rem;
    }
  }
{%- endstyle -%}

